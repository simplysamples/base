body{margin:0}*,*::before,*::after{box-sizing:border-box}textarea{resize:vertical}table{border-collapse:collapse}img,svg,video,canvas,audio,iframe,embed,object{display:block;vertical-align:middle}img,video{max-width:100%;height:auto}html{scroll-behavior:smooth}.sr-only{position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);white-space:nowrap;border-width:0}:root{--sm:768px;--md:1000px}.px-sm{padding:0 1rem}.p-sm{padding:1rem}.py-sm{padding:1rem 0}@media screen and (--sm){.py-sm{padding:2rem 0}}.mx-auto{margin-right:auto;margin-left:auto}.space-y-sm>*+*{margin-top:1rem}body{font-size:1.2rem}h1,h2,h3,h4,h5,h6,p,ul,ol,blockquote{margin-top:1.5em}*:first-child{margin-top:0}h1{font-size:4rem}h2{font-size:3.5rem}h3{font-size:3rem}h4{font-size:2.5rem}h5{font-size:2rem}h6{font-size:1.5rem}.text-center{text-align:center}.container{max-width:65ch;margin:0 auto;padding:0 1rem}.flex{display:flex}.flex-1{flex:1}.direction-column{flex-direction:column}.align-start{align-items:flex-start}.align-center{align-items:center}.align-end{align-items:flex-end}.justify-start{justify-content:flex-start}.justify-center{justify-content:center}.justify-end{justify-content:flex-end}.justify-around{justify-content:space-around}@media screen and (max-width:var(--sm)){.row-reverse-sm{flex-direction:row-reverse}}:root{--light:aliceblue;--dark:#43bbaddb}.bg-dark{color:var(--light);background-color:var(--dark)}.btn,.textInput,select{border-width:2px;border-style:solid;padding:.5rem 1rem;border-radius:.5rem;font-size:1rem;line-height:1.2}.btn{display:inline-block}.textInput,select{width:100%;display:block}.mobileMenuIcon{position:relative;display:block;height:1.7rem;width:2.55rem}.mobileMenuIcon span{display:block;position:absolute;left:0;right:0;height:.3rem;background-color:currentColor}.mobileMenuIcon.is-bars span:first-child{top:0}.mobileMenuIcon.is-bars span:nth-child(2){top:50%;transform:translateY(-50%)}.mobileMenuIcon.is-bars span:last-child{bottom:0}.m